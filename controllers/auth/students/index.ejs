<!-- students/index.ejs -->
<!DOCTYPE html>
<html>
<head>
  <title>List of Students</title>
</head>
<body>
  <h1>List of Students</h1>
  <!-- Add the list of students here -->
  <ul>
    <% students.forEach((student) => { %>
      <li><%= student.name %> - <%= student.college %> - Status: <%= student.status %></li>
    <% }); %>
  </ul>
</body>
</html>
